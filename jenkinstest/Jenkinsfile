pipeline {
    agent any

    stages {
        stage ('Compile Stage') {

            steps {
                withMaven(maven : 'Maven_Home') {
                    shell 'mvn clean compile'
                }
            }
        }
    }
    
    
    stage ('Testing Stage') {

            steps {
                withMaven(maven : 'Maven_Home') {
                    shell 'mvn test'
                }
            }
        }
        
        
}